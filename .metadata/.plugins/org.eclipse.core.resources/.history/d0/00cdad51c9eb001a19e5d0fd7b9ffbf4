package aplicacao;

import dominio.Pessoa;

public class Programa {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa(1L, "Daniel Silva", "daniel@gmail.com"); 
		Pessoa p2 = new Pessoa(1L, "Maria Silva", "maria@gmail.com"); 
		Pessoa p3 = new Pessoa(1L, "Joao Silva", "joao@gmail.com"); 
        System.out.println(p1); 
        System.out.println(p2); 
        System.out.println(p3); 


	}

}
